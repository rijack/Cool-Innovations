<tr class="nested-fields">
  <td><%= f.association :part, :label =>false, :collection => Part.order(:part_number).all, :input_html => {:"data-placeholder" => "Part #", :class => "span2"} %></td>
  <td><%= f.input :new_part_number, :label =>false ,:input_html => {:class => "span1"} %></td>
  <td><%= f.input :quantity, :label =>false,:input_html => {:class => "span1"} %></td>
  <td>
    <%= link_to_remove_association  f, :class => "btn btn-small btn-danger", :title => "Remove Line" do%>
        <i class="icon-trash" alt="edit"></i>
    <% end %>
  </td>
</tr>